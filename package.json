{
  "name": "styleguide",
  "description": "Generate a styleguide from Compositor Lab configs",
  "author": "Compositor",
  "version": "0.0.1",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "demo": "x0 dev src/Page.js",
    "build": "babel src -d dist",
    "comp": "lab --pkg -d library",
    "dev": "rm -rf out && npm run build && ./dist/cli.js docs/components -d out && superstatic out",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "ava -v"
  },
  "repository": "c8r/styleguide",
  "keywords": [
    "styleguide",
    "design",
    "system",
    "react",
    "compositor",
    "lab"
  ],
  "license": "MIT",
  "dependencies": {
    "@compositor/gen": "github:c8r/gen#md",
    "@compositor/globb": "^0.1.7",
    "@compositor/markdown": "0.0.18",
    "@compositor/x0": "github:c8r/x0#raw-loader",
    "chalk": "^2.3.0",
    "dot-prop": "^4.2.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "meow": "^4.0.0",
    "react": "^16.2.0",
    "react-docgen": "^2.20.0",
    "react-dom": "^16.2.0",
    "react-live": "^1.7.1",
    "react-x-ray": "^1.0.0-4",
    "styled-parser": "^1.0.0-0",
    "styled-system": "^1.1.1"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "execa": "^0.9.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
