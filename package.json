{
  "name": "@compositor/styleguide",
  "description": "Generate a styleguide from Compositor Lab configs",
  "author": "John Otander",
  "version": "0.0.7",
  "main": "dist/index.js",
  "bin": {
    "styleguide": "dist/cli.js"
  },
  "files": [
    "dist",
    "library"
  ],
  "scripts": {
    "babel": "babel src -d dist",
    "build": "npm run babel && npm run comp",
    "clean": "rm -rf dist && rm -rf library",
    "comp": "lab --pkg -d library",
    "prepare": "npm run clean && npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "x0 dev src",
    "test": "ava -v"
  },
  "repository": "c8r/styleguide",
  "keywords": [
    "styleguide",
    "design",
    "system",
    "react",
    "compositor",
    "lab"
  ],
  "license": "MIT",
  "dependencies": {
    "@compositor/globb": "^0.1.7",
    "@compositor/markdown": "0.0.21",
    "@compositor/x0": "^3.1.1",
    "chalk": "^2.3.0",
    "colorable": "^1.0.5",
    "dot-prop": "^4.2.0",
    "gray-matter": "^3.1.1",
    "meow": "^4.0.0",
    "prop-types": "^15.6.0",
    "react-docgen": "^2.20.0",
    "react-router-dom": "^4.2.2",
    "react-x-ray": "^1.0.0-4",
    "styled-components": "^3.1.4",
    "styled-parser": "^1.0.0-0",
    "styled-system": "^1.1.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "execa": "^0.9.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
